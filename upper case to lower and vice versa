#include<bits/stdc++.h>
using namespace std;

int main()
{   //changing from upper case to lower case
    char A[] = "WELCOME";
    int i;
    for ( i = 0; A[i] != '\0'; i++)
    {
        A[i] += 32;
    }
    cout<<A;
    //changing from lower case to upper case
    char A[] = "welcome";
    int i;
    for ( i = 0; A[i] != '\0'; i++)
    {
        A[i] -= 32;
    }
    cout<<A;
    return 0 ;
}

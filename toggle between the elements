#include<bits/stdc++.h>
using namespace std;

int main()
{   //toggle between the elements
    char A[] = "wElCome";
    
    int i;
    for ( i = 0; A[i] != '\0'; i++)
    {
        if(A[i] >='A' && A[i]<='Z')
            A[i] += 32;
        else if(A[i]>='a' && A[i]<=122)
            A[i] -= 32;
    }
    cout<<A;
    return 0 ;
}
